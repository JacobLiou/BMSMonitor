                    <!--单体超限/低-->
                    <GroupBox Header="单体超限/低" Grid.Row="0" Grid.Column="2" Margin="5 10">
                        <Grid Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
