                            <TextBox x:Name="txtAlarmParameters_3_1" Text="{Binding AlarmParameter_3_1}" Width="80" Margin="10 0" Grid.Column="2" Grid.Row="0" TextAlignment="Center" VerticalAlignment="Center"/>

                            <Label Content="单体过放解除 (V)" Grid.Column="1" Grid.Row="1" VerticalAlignment="Bottom"/>
                            <TextBox x:Name="txtAlarmParameters_3_2" Text="{Binding AlarmParameter_3_2}" Width="80" Grid.Column="2" Grid.Row="1" TextAlignment="Center" VerticalAlignment="Center"/>

                            <Label Content="单体过放告警 (V)" Grid.Column="1" Grid.Row="2" VerticalAlignment="Bottom"/>
                            <TextBox x:Name="txtAlarmParameters_3_3" Text="{Binding AlarmParameter_3_3}" Width="80" Grid.Column="2" Grid.Row="2" TextAlignment="Center" VerticalAlignment="Center"/>

                            <Label Content="单体过放解除 (V)" Grid.Column="1" Grid.Row="3" VerticalAlignment="Bottom"/>
                            <TextBox x:Name="txtAlarmParameters_3_4" Text="{Binding AlarmParameter_3_4}" Width="80" Grid.Column="2" Grid.Row="3" TextAlignment="Center" VerticalAlignment="Center"/>
                        </Grid>
                    </GroupBox>

                    <!--总体过放-->
                    <GroupBox Header="总体过放" Grid.Row="3" Grid.Column="0" Margin="5 10">
                        <Grid Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="150" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="35"/>
                                <RowDefinition Height="35"/>
                                <RowDefinition Height="35"/>
                                <RowDefinition Height="35"/>
                            </Grid.RowDefinitions>

                            <CheckBox x:Name="ckbAlarmParameters_4" IsChecked="{Binding IsChecked_AlarmParameter_4}" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 10 0 0"/>
                            <Label Content="总体过放保护 (V)" Grid.Column="1" VerticalAlignment="Bottom"/>
