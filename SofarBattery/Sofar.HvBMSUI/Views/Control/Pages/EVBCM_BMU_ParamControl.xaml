<UserControl x:Class="Sofar.HvBMSUI.Control.Pages.EVBCM_BMU_ParamControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Sofar.HvBMSUI.Control.Pages" 
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" 
             xmlns:viewmodels="clr-namespace:Sofar.HvBMSUI.ViewModels" 
             d:DataContext="{d:DesignInstance Type=viewmodels:BMU_ParamControl_ViewModel}" mc:Ignorable="d"  
             d:DesignHeight="1082" d:DesignWidth="2010">

    <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
        <Grid Background="White">
            <Grid Margin="0,10,0,0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <!-- 分隔线 -->
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>


                <!-- 左侧 GroupBox 区域 -->

                <GroupBox Grid.Row="0" Header="报警参数设置" FontSize="15"  Margin="10,0,0,0">
                    <StackPanel >
                        <StackPanel Orientation="Horizontal" >

                            <Label Content="BMU设备地址" FontSize="16" Foreground="OrangeRed"    Margin="0,0,10,0"  VerticalAlignment="Center" />
                            <ComboBox x:Name="ckbAddress_BMU"  FontSize="16" BorderBrush="OrangeRed" Foreground="OrangeRed" Width="100"  Margin="0,0,10,0" BorderThickness="1" 
                              ItemsSource="{Binding Address_BMU_List}"   SelectedItem="{Binding SelectedAddress_BMU,Mode=TwoWay}">
                                <ComboBox.ToolTip>
                                    <ToolTip Background="LightGreen" Placement="MousePoint"  Foreground="Black">
                                        切换此地址再点击读取按钮，可获取对应BMU设备的报警参数
                                    </ToolTip>
                                </ComboBox.ToolTip>
                            </ComboBox>

                            <Label Content="软件版本号"  VerticalAlignment="Center" />
                            <TextBox Margin="20,0,0,0"  IsReadOnly="True" x:Name="txtSoftwareVersion" Text="{Binding SoftwareVersion,UpdateSourceTrigger=PropertyChanged}" Width="80" Grid.Column="1" TextAlignment="Center"/>

                            <Label Content="硬件版本号"    VerticalAlignment="Center" />
                            <TextBox Margin="20,0,10,0"  IsReadOnly="True"  x:Name="txtHardwareVersion" Text="{Binding HardwareVersion,UpdateSourceTrigger=PropertyChanged}" Width="80" Grid.Column="1" TextAlignment="Center"/>
                            <Button x:Name="btnAlarmParametersSetting_3" Width="200" Content="恢复从控报警参数默认值" Margin="0,0,10,0" Command="{Binding InitCmd}"/>

                            <Button x:Name="btnAlarmParametersSetting_4" Width="200" Content="导入从控报警参数配置"   Margin="0,0,10,0" Command="{Binding ImportCmd}"/>

                            <Button x:Name="btnAlarmParametersSetting_5" Width="200" Content="导出从控报警参数配置"   Margin="0,0,10,0" Command="{Binding ExportCmd}"/>

                        </StackPanel>
                        <StackPanel Orientation="Horizontal" >
                            <Button x:Name="btnAlarmParametersSetting_1" Width="200" Content="读取从控报警参数"   Margin="0,0,10,0" Command="{Binding ReadCmd}"/>

                            <Button x:Name="btnAlarmParametersSetting_2" Width="200" Grid.Column="0" Content="下设从控报警参数" Margin="0,0,10,0" Command="{Binding WriteCmd}"/>
                            <CheckBox x:Name="ckbSelectAll"  Margin="20,0,0,0" Grid.Column="1" IsChecked="{Binding IsAllChecked}"/>
                            <Label Content="全选"  Margin="0,0,0,0"  Grid.Column="2" VerticalContentAlignment="Center"/>

                        </StackPanel>

                    </StackPanel>
                </GroupBox>

                <!-- 右侧 4 行 5 列 GroupBox 布局 -->
                <Grid Grid.Row="1" Margin="10,0">
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>

                    <!--单体过充-->
                    <GroupBox Header="单体过充" Grid.Row="0" Grid.Column="0" Margin="5 10">
                        <Grid Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="150" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="35"/>
                                <RowDefinition Height="35"/>
                                <RowDefinition Height="35"/>
                                <RowDefinition Height="35"/>
                            </Grid.RowDefinitions>

                            <CheckBox x:Name="ckbAlarmParameters_1" IsChecked="{Binding IsChecked_AlarmParameter_1}" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 10 0 0"/>
                            <Label Content="单体过充保护 (mV)" Grid.Column="1" VerticalAlignment="Bottom"/>
                            <TextBox x:Name="txtAlarmParameters_1_1" Text="{Binding AlarmParameter_1_1,UpdateSourceTrigger=PropertyChanged}" Width="80" Margin="10 0" Grid.Column="2" TextAlignment="Center" VerticalAlignment="Center"/>

                            <Label Content="单体过充解除 (mV)" Grid.Column="1" Grid.Row="1" VerticalAlignment="Bottom"/>
                            <TextBox x:Name="txtAlarmParameters_1_2" Text="{Binding AlarmParameter_1_2}" Width="80" Grid.Column="2" Grid.Row="1" TextAlignment="Center" VerticalAlignment="Center"/>

                            <Label Content="单体过充告警 (mV)" Grid.Column="1" Grid.Row="2" VerticalAlignment="Bottom"/>
                            <TextBox x:Name="txtAlarmParameters_1_3" Text="{Binding AlarmParameter_1_3}" Width="80" Grid.Column="2" Grid.Row="2" TextAlignment="Center" VerticalAlignment="Center"/>

                            <Label Content="单体过充解除 (mV)" Grid.Column="1" Grid.Row="3" VerticalAlignment="Bottom"/>
                            <TextBox x:Name="txtAlarmParameters_1_4" Text="{Binding AlarmParameter_1_4}" Width="80" Grid.Column="2" Grid.Row="3" TextAlignment="Center" VerticalAlignment="Center"/>
                        </Grid>
                    </GroupBox>

                    <!--总体过充-->
                    <GroupBox Header="总体过充" Grid.Row="1" Grid.Column="0" Margin="5 10">
                        <Grid Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="150" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="35"/>
                                <RowDefinition Height="35"/>
                                <RowDefinition Height="35"/>
                                <RowDefinition Height="35"/>
                            </Grid.RowDefinitions>

                            <CheckBox x:Name="ckbAlarmParameters_2" IsChecked="{Binding IsChecked_AlarmParameter_2}" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 10 0 0"/>
                            <Label Content="总体过充保护 (V)" Grid.Column="1" VerticalAlignment="Bottom"/>
                            <TextBox x:Name="txtAlarmParameters_2_1" Text="{Binding AlarmParameter_2_1}" Width="80" Margin="10 0" Grid.Column="2" Grid.Row="0" TextAlignment="Center" VerticalAlignment="Center"/>

                            <Label Content="总体过充解除 (V)" Grid.Column="1" Grid.Row="1" VerticalAlignment="Bottom"/>
                            <TextBox x:Name="txtAlarmParameters_2_2" Text="{Binding AlarmParameter_2_2}" Width="80" Grid.Column="2" Grid.Row="1" TextAlignment="Center" VerticalAlignment="Center"/>

                            <Label Content="总体过充告警 (V)" Grid.Column="1" Grid.Row="2" VerticalAlignment="Bottom"/>
                            <TextBox x:Name="txtAlarmParameters_2_3" Text="{Binding AlarmParameter_2_3}" Width="80" Grid.Column="2" Grid.Row="2" TextAlignment="Center" VerticalAlignment="Center"/>

                            <Label Content="总体过充解除 (V)" Grid.Column="1" Grid.Row="3" VerticalAlignment="Bottom"/>
                            <TextBox x:Name="txtAlarmParameters_2_4" Text="{Binding AlarmParameter_2_4}" Width="80" Grid.Column="2" Grid.Row="3" TextAlignment="Center" VerticalAlignment="Center"/>
                        </Grid>
                    </GroupBox>

                    <!--单体过放-->
                    <GroupBox Header="单体过放" Grid.Row="2" Grid.Column="0" Margin="5 10">
                        <Grid Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="150" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
