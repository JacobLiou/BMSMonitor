                     </RadioButton.Style>-->
                                            </RadioButton>
                                            <RadioButton Margin="50,0,0,0" Content="开" Background="Red" IsChecked="{Binding IsChecked_DO8_Open}"/>
                                        </StackPanel>
                                    </GroupBox>

                                    <!-- 控制 -->
                                    <StackPanel  Grid.Row="0" Grid.Column="2"  Orientation="Horizontal" Margin="0,10,0,0">
                                        <Button Margin="50,25,0,0" Content="DO全开" Command="{Binding FullyOpen_DO_Cmd}" Width="90">
                                        </Button>
                                    </StackPanel>
                                    <StackPanel  Grid.Row="1" Grid.Column="2"  Orientation="Horizontal" Margin="0,10,0,0">
                                        <Button Margin="50,25,0,0" Content="DO全关" Command="{Binding FullyClose_DO_Cmd}" Width="90">
                                        </Button>
                                    </StackPanel>
                                    <StackPanel  Grid.Row="2" Grid.Column="2"  Orientation="Horizontal" Margin="0,10,0,0">
                                        <Button Margin="50,25,0,0" Content="下发控制" Command="{Binding DistributeControlCmd}" Width="90">
                                        </Button>
                                    </StackPanel>

                                </Grid>
                            </GroupBox>
                            <GroupBox Header="控制主控上下电指令" Margin="0,10,0,5" BorderBrush="Gray" BorderThickness="2" Style="{x:Null}">
                                <StackPanel Orientation="Horizontal" Margin="5,20,0,20">
                                    <Label Content="上下电" Margin="5" Width="60"/>
                                    <ComboBox   x:Name="cbbPowerOnAndOff"    Width="150"  Margin="5,0,0,0"  BorderThickness="1" ItemsSource="{Binding PowerOnAndOffList  }"   SelectedItem="{Binding SelectedPowerOnAndOff,Mode=TwoWay}"/>
                                    <Label Content="操作簇说明" Margin="5" Width="80"/>
                                    <ComboBox   x:Name="cbbOperatingInstructions"    Width="200"  Margin="5,0,0,0"  BorderThickness="1" ItemsSource="{Binding OperatingInstructionsList}"   SelectedItem="{Binding SelectedOperatingInstructions,Mode=TwoWay}"/>
                                    <Label Content="故障复归" Margin="5" Width="60"/>
                                    <ComboBox   x:Name="cbbFaultReset"    Width="150"  Margin="5,0,0,0"  BorderThickness="1" ItemsSource="{Binding FaultResetList}"   SelectedItem="{Binding SelectedFaultReset,Mode=TwoWay}"/>
                                    <Button Margin="50,0,0,0" Content="下发指令" Command="{Binding PowerOnAndOffControlCmd}" Width="90"></Button>
                                </StackPanel>
                            </GroupBox>
                            <GroupBox Header="控制主控绝缘检测功能指令" Margin="0,10,0,5" BorderBrush="Gray" BorderThickness="2" Style="{x:Null}">
                                <StackPanel Orientation="Horizontal" Margin="5,20,0,20">
                                    <Label Content="主控绝缘检测指令" Margin="5,10,0,0" Width="120"/>
                                    <ComboBox   x:Name="cbbInsulationDetection"    Width="150"  Margin="5,10,0,0"  BorderThickness="1" ItemsSource="{Binding InsulationDetectionList}"   SelectedItem="{Binding SelectedInsulationDetection,Mode=TwoWay}"/>
                                    <Button Margin="10,5,0,0" Content="下发指令" Command="{Binding InsulationDetectionCmd}" Width="90"></Button>
                                    <Label Content="主控绝缘检测状态" Margin="50,10,0,0" Width="110"/>
                                    <ComboBox   x:Name="cbbInsulationDetectionStatus" IsEnabled="False"   Width="150"  Margin="0,10,0,0"  BorderThickness="1" ItemsSource="{Binding InsulationDetectionStatusList}"   SelectedItem="{Binding InsulationDetectionStatus,Mode=TwoWay}"/>
                                </StackPanel>
                            </GroupBox>

                            <GroupBox Header="自动分配地址" Margin="0,10,0,10" BorderBrush="Gray" BorderThickness="2" Style="{x:Null}">
                                <StackPanel Orientation="Horizontal" Margin="5,20,0,20">
                                    <Label Content="系统模块总个数" Margin="5,10,0,0" Width="120"/>
                                    <TextBox Margin="5,0,0,5" Text="{Binding TotalNumberofSystemModules,UpdateSourceTrigger=PropertyChanged}" Width="150" TextAlignment="Center"  VerticalContentAlignment="Center"/>
                                    <Button Margin="10,5,0,0" Content="自动分配地址" Command="{Binding AutomaticAllocationAddrCmd}" Width="120"></Button>
                                </StackPanel>
                            </GroupBox>
                        </StackPanel>
                    </GroupBox>

                </StackPanel>
            </Grid>
        </Grid>
    </ScrollViewer>
</UserControl>
